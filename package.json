{
  "name": "project-for-typescript",
  "version": "1.0.0",
  "description": "A typscript and graphql practice",
  "main": "server.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'"
  },
  "keywords": [
    "Tyscript",
    "Graphql",
    "node",
    "javascript"
  ],
  "author": "Hassan Ahmad",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.7",
    "@types/mocha": "^8.0.0",
    "@types/mongoose": "^5.7.32",
    "@types/node": "^14.0.23",
    "@types/node-schedule": "^1.3.0",
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@typegoose/typegoose": "^6.5.0",
    "apollo-boost": "^0.4.9",
    "apollo-server-express": "^2.15.1",
    "class-validator": "^0.11.1",
    "cross-fetch": "^3.0.5",
    "express": "^4.17.1",
    "graphql": "^14.7.0",
    "mongoose": "^5.9.24",
    "node-schedule": "^1.3.2",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^0.17.6"
  }
}
